    "tslint": "^4.1.1",
    "tslint-microsoft-contrib": "^4.0.0",
    "typescript": "^2.1.4"
  },
  "dependencies": {
    "@types/express": "^4.0.34",
    "@types/mongoose": "^4.7.1",
